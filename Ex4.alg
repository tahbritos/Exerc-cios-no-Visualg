algoritmo "ParImparProcedimento"


var
      N: Inteiro
      
Procedimento ParOuImpar(V: Inteiro)
Inicio
     Se (V%2 = 0) entao
        Escreval("O numero ", v, " e Par")
     Senao
        EscrevaL ("O numero ", v, " e Impar")
     FimSe
FimProcedimento

inicio
      Escreva("Digite um numero: ")
      Leia (N)
      ParOuImpar(N)


fimalgoritmo
