algoritmo "Qual_idade"

var
   Idade, Anonascimento, Anoatual: inteiro

   
inicio
  Escreva ("Ola, tudo bem? Em que ano estamos? ")
  Leia (Anoatual)
  
  Escreva ("Qual ano voce nasceu? ")
  Leia (Anonascimento)
  Idade <- Anoatual - Anonascimento

  Escreva ("Sua idade e ", Idade, " anos")

fimalgoritmo
